Version 2
Destination 0xc

#Request Version and status info from testserver
Message SystemInfo 0x0001  ReplySystemInfo   #No arguments
end

#Set logging level, higher = more
Message LogLevel 0x0002
    Level UInt
end

#Load FPGA image (from those already available on testset)
Message LoadImage 0x0003
    FileName String
end

#Get MAX10 firmware version
Message GetRFVersion 0x0004
end

#Get FPGA firmware version
Message GetFpgaVersion 0x0005 ReplyFpgaVersion 
end

#Enable/disable logging for specific modules
#Bits to set (or clear)
# SYSINFO =     0x1;
# SCOPE =       0x2;
# RFCONTROL =   0x4;
# UAT =         0x8;
# ADSB1090 =    0x10;
Message LogModule 0x0006 
    SetBits UInt     #Modules to be logged See Log.h for module bit definitions
    ClearBits UInt   #Modules to inhibit logging 
end

Message ServerExit 0x0007
    Exit Bool
end

Message SetVerbose 0x0008
    Verbose Bool
end

Message VersionsSummary 0x0009 ReplyVersionsSummary
end